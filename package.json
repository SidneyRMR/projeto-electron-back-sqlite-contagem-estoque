{
  "name": "estoque-contagem",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "electron": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "next build && electron-builder"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "react": "^18.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-modal": "^3.16.1",
    "react-qr-code": "^2.0.14",
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^31.0.0",
    "electron-builder": "^24.6.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "14.2.3",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.estoque.contagem",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "preload.js",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
